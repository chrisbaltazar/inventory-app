{% extends 'base.html.twig' %}

{% block title %}Suit{% endblock %}

{% block body %}
    <h1>Suit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ suit.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ suit.name }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ suit.region }}</td>
            </tr>
            <tr>
                <th>Gender</th>
                <td>{{ suit.gender }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ suit.updatedAt ? suit.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DeletedAt</th>
                <td>{{ suit.deletedAt ? suit.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_suit_index') }}">back to list</a>

    <a href="{{ path('app_suit_edit', {'id': suit.id}) }}">edit</a>

    {{ include('suit/_delete_form.html.twig') }}
{% endblock %}
